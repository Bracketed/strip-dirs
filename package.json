{
  "name": "strip-dirs",
  "version": "1.1.1",
  "description": "Remove leading directory components from a path, like tar's --strip-components option",
  "repository": "shinnn/node-strip-dirs",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "bin": "cli.js",
  "files": [
    "cli.js",
    "index.js"
  ],
  "scripts": {
    "pretest": "eslint --config node_modules/@shinnn/eslintrc-node/rc.json --rule 'no-reserved-keys: 0' --rule 'no-process-exit: 0' --ignore-path .gitignore .",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "license": "MIT",
  "keywords": [
    "path",
    "dir",
    "directory",
    "strip",
    "strip-components"
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "get-stdin": "^4.0.1",
    "is-absolute": "^0.1.7",
    "is-natural-number": "^2.0.0",
    "minimist": "^1.1.1",
    "sum-up": "^1.0.2"
  },
  "devDependencies": {
    "@shinnn/eslintrc-node": "^1.0.2",
    "eslint": "^0.23.0",
    "istanbul": "^0.3.15",
    "istanbul-coveralls": "^1.0.3",
    "tap-spec": "^4.0.0",
    "tape": "^4.0.0"
  }
}
